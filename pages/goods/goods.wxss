  background: #3d4663;
  border-radius: 0;
  text-align: center;
  line-height: 78rpx;
  float: right;
}

/* 列表样式 */

.item_box {
  margin-bottom: 28.125rpx;
  min-height: 200.625rpx;
  box-sizing: border-box;
  padding: 20.625rpx;
  display: flex;
  background-color: #fff;
  border-radius: 20.625rpx;
}

.item_l {
  flex-shrink: 0;
  margin-right: 22.5rpx;
  border-radius: 20.625rpx;
  overflow: hidden;
}

.item_r {
  flex-grow: 1;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.item_img {
  height: 157.5rpx;
  width: 251.25rpx;
  display: block;
}

.it_row_1 {
  width: 100%;
  display: flex;
  justify-content: space-between;
}

.item_t1 {
  display: -webkit-box;
  /**对象作为伸缩盒子模型展示**/
  -webkit-box-orient: vertical;
  /**设置或检索伸缩盒子对象的子元素的排列方式**/
  -webkit-line-clamp: 2;
  /**显示的行数**/
  overflow: hidden;
  /**隐藏超出的内容**/

  font-size: 26.25rpx;
  font-family: Source Han Sans CN;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
}

.bold {
  font-size: 36.875rpx;
  font-weight: bold;
}

.item_t2,
.it_row_2 {
  font-size: 20.625rpx;
  font-family: Source Han Sans CN;
  font-weight: 400;
  color: rgba(102, 102, 102, 1);
}

.it_row_3 {
  margin-left: auto;
  display: flex;
  justify-content: flex-end;
}

.del_but {
  display: flex;
  align-items: center;
  height: 39.375rpx;
  border: 1px solid rgba(218, 182, 143, 1);
  border-radius: 18.75rpx;
  font-size: 20.625rpx;
  padding: 0rpx 13.125rpx;
  font-family: Source Han Sans CN;
  font-weight: 400;
  color: rgba(218, 182, 143, 1);
}