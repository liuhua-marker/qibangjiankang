/* pages/speechTraining/speechTraining.wxss */
page{
   height: 100%;

}
.panel{
  padding:30rpx 30rpx 200rpx 30rpx;
  width:auto;
  height: 100%;
}
.content_panel{
  background:#fff;
  border-radius: 10rpx;
  box-shadow: 0 0 6px 4px rgba(0, 0, 0, 0.1);
}
.title{
  text-align: center;
  font-size: 36rpx;
  color:#333;
  padding:30rpx 0 ;
  border-bottom: 1px solid #dcdcdc;
}
.bage_panel{
  text-align: right;
  padding:30rpx;
}
.bage{
  display: inline-block;
  /* padding:0 30rpx; */
  height: 60rpx;
  line-height: 60rpx;
  border-radius: 60rpx;
  /* background: #F46F38; */
  color:#FF6C00;
  font-size: 30rpx;
  margin-left: 40rpx;
}
.content_box{
  padding:0 20rpx 0 20rpx;
  color:#666666;
  line-height: 44rpx;
  font-size: 32rpx;
  text-indent: 60rpx;
}
.bottom_panel{
  font-size: 30rpx;
  text-align: right;
  color:#333;
  padding:30rpx;

}
.color_o{
  color:#BD4044;
  padding-left: 10rpx;
}
.btn_box{
  position: fixed;
  bottom:40rpx;
  width: 100%;
  min-height: 40rpx;
  left: 0;
  z-index: 12;
  display: flex;
  color:#444;
}
.btn_box>view{
  flex-grow: 1;
  text-align: center;
}
.btn_bottom{
  font-size: 34rpx;
  /* padding:0 80rpx; */
}
.triangle_style{
    width:80rpx;
    height: 80rpx;
    display: inline-block;
}
.triangle_left{
  border-right-color: #3897DE;
  /* margin-right: 40rpx; */
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100%;
}
.triangle_right{
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100%;
}
.btn_bottom{
  margin-top:20rpx;
  position: relative;
}
.btn_read{
  width:140rpx;
  height: 140rpx;
  display: inline-block;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100% 100%;
}
.voice{
  position: absolute;
  width:120rpx;
  height: 120rpx;
  /* background: red; */
  top:-140rpx;
  left: 50%;
  transform: rotate(-90deg);
  margin-left: -60rpx;
   animation: test 2s linear infinite;
      background: url(data:image/png;base64,) no-repeat;
      background-size: 100%;
  /* animation-direction:alternate; */
}
@keyframes test{
  0%{
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100%;
  }
  50%{
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100% ;
  }
  100%{
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100%;
  }
}