.bg_F8{
  background: #F7F4F8 url(data:image/png;base64,) no-repeat;
  background-size: 100%;
}
.top_panel {
  height: 340rpx;
  background-size: contain;
  max-width: 100%;
  text-align: center;
  position: relative;
  z-index: 10;
}
.mine_banner {
  position: absolute;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  z-index: 0;
}
.center_box {
  position: absolute;
  width: cacl(100% - 60rpx);
  left: 30rpx;
  right:30rpx;
  min-height: 100rpx;
  border-radius: 10rpx;
  background: #fff;
  padding: 40rpx 0;
  box-shadow: 0 0 2px 2px rgba(0, 0, 0, 0.1);
}
/* 三角形 */
.triangle{
  width: 0; 
  height: 0;
  border:10rpx solid transparent;
  border-top-color: #C26164;
  display: inline-block;
  margin:0 10rpx;
  vertical-align: text-bottom;
}
/* 左右线条 */
.line_style{
  display: inline-block;
  position: relative;
}
.line_style::after,.line_style::before{
  content: "";
  position: absolute;
  width:100rpx;
  border-top:1px solid #DCDCDC;
  top:25rpx;
}
.line_style::after{
  left: -130rpx;
}
.line_style::before{
  right:-130rpx
}
.color_333{
  color:#333
}
.color_666{
  color:#666
}
.color_red{
  color:#BD4044
}
.color_blue{
  color:#0066CC
}
/* 弹框 */
.dialog{
  position: fixed;
  top:0;
  left: 0;
  right: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.5);
  z-index: 1000;
}
.dialog .dialog_panel{
  position: absolute;
  margin:0;
  left:30rpx;
  right: 30rpx;
  top:25%;
  height: 50%;
  background: #fff;
}
.dilog_title{
  font-weight: bold;
  height: 100rpx;
  line-height: 100rpx;
}
.close_style{
  width: 50rpx;
  height: 50rpx;
  float: right;
  margin:25rpx 30rpx 0 0;
}
.dialog_panel .flex_top{
  text-align: center;
  margin: 0;
  border-bottom: 1px solid #DCDCDC;
  padding:0;
}
.dialog .flex_table_item{
  width: 33.3%;
  line-height: 100rpx;
}

.select_panel{
  background: rgba(0, 0, 0, 0.3);
  color:#FEFEFE;
  height: 60rpx;
  border-radius: 30rpx;
  padding:0 20rpx;
  color:#fff;
  border:1px solid #FEFEFE;
  line-height: 60rpx;
}
.select_panel .triangle{
  border-top-color:#fff;
}
.btn_group_panel {
  padding: 30rpx 0 0 0;
}

.btn_group {
  display: flex;
  text-align: center;
  height: 80rpx;
  line-height: 80rpx;
  background: #fff;
  border-radius: 14rpx;
  box-shadow: 0 0 2px 2px rgba(0, 0, 0, 0.1);
}

.btn_group_item {
  flex-grow: 1;
  color: #282B30;
  font-size: 32rpx;
  /* font-weight: bold; */
  border-radius: 14rpx;
}

.btn_group_item.active {
  background: #B73F44;
  color: #fff;
}
.load_more{
  padding:30rpx;
  font-size: 30rpx;
  color:#666;
  text-align: center;
}
.empty-view {
  height: 100%;
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;

}

.empty-view .icon {
  height: 120rpx;
  width: 120rpx;
  margin-bottom: 10rpx;
}
.no_data{
  color:#666;
  font-size: 34rpx;
  padding:30rpx 0;
  text-align: center;
}